{
  "manifest_version": 2,

  "name": "Focus",
  "description": "This extension locks your browser to the current page",
  "version": "1.0",
   "browser_action": {
    "default_icon": "unlocked.png",
    "default_popup":"popup.html"
  },
  "permissions": ["activeTab","webRequest","tabs" ,"webRequestBlocking","storage","management",
                 "https://*/","https://*/*" ,"http://*/","http://*/*"],
  "background": {
    "scripts": [ "background.js", "jquery-3.1.1.min.js"]
  }

}